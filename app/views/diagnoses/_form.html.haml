/ end page title
.row
  .col-12
    .card
      .card-body
        = form_for @diagnosis, class: 'form-horizontal' do |f|
          .mb-3
            = f.label :name, class: 'form-label'
            = f.text_field :name, class: 'form-control'
          .mb-3
            = f.submit "Save", class: 'btn btn-success btn-block'
